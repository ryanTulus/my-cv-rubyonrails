<!DOCTYPE html>
<html>
<head>
  <title>Mycv</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <br/>
  <% if user_signed_in? %>
    <div class="col-md-12 nav">
      <%= link_to "Logout", destroy_user_session_path, method: :delete, class: 'btn btn-warning pull-right' %>
    </div>
  <% end %>
<%= yield %>

</body>
</html>
